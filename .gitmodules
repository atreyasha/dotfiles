[submodule "i3/.config/i3/i3blocks-contrib"]
  path = conf/i3/.config/i3/i3blocks-contrib
  url = https://github.com/vivien/i3blocks-contrib.git
[submodule "vim/.vim_runtime"]
  path = conf/vim/.vim_runtime
  url = https://github.com/amix/vimrc.git
[submodule "ranger/.config/ranger/plugins/ranger_devicons"]
